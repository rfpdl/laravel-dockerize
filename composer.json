{
  "name": "rfpdl/laravel-dockerize",
  "description": "Laravel-specific Docker scaffolding installer with dev/local/production presets.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {"name": "Roy Francis Ponce de Leon", "email": "rfpdl@outlook.com"}
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0 || ^11.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelDockerize\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelDockerize\\ServiceProvider"
      ]
    }
  },
  "bin": [],
  "minimum-stability": "stable",
  "prefer-stable": true
}
